def insertUser(username, password):
    db = sql.connect(f)
    c = db.cursor()
    c.execute("INSERT INTO users(username,password) VALUES (?,?)", (username,password))
    db.commmit()
    db.close()

def retrieveUsers():
    db = sql.connect(f)
    c = db.cursor()
    c.execute("SELECT username, password FROM users")
    users = c.fetchall()
    db.close()
    return users
